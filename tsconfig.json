{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,                        /* Redirect output structure to the directory. */
    "esModuleInterop": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts", "app/_layout.tsx"]
}
